package uml;

import javax.swing.JFrame;
import javax.swing.JTabbedPane;
import java.awt.BorderLayout;
import javax.swing.JPanel;

public class m extends JFrame{
	public m() {
		this.setSize(200,400);
		JTabbedPane tabbedPane = new JTabbedPane(JTabbedPane.TOP);
		getContentPane().add(tabbedPane, BorderLayout.CENTER);
		
		JPanel AddDoctor = new JPanel();
		tabbedPane.addTab("New tab", null, AddDoctor, null);
		
		JPanel AddNurse = new JPanel();
		tabbedPane.addTab("New tab", null, AddNurse, null);
		
		JPanel PatientsVisitedDoctors = new JPanel();
		tabbedPane.addTab("New tab", null, PatientsVisitedDoctors, null);
		
		JPanel PatientWaitngTime = new JPanel();
		tabbedPane.addTab("New tab", null, PatientWaitngTime, null);
	}
}

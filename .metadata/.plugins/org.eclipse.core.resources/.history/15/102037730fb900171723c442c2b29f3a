			groupLayout.createParallelGroup(Alignment.LEADING)
				.addGap(0, 500, Short.MAX_VALUE)
		);
		groupLayout.setVerticalGroup(
			groupLayout.createParallelGroup(Alignment.LEADING)
				.addGap(0, 500, Short.MAX_VALUE)
		);
		getContentPane().setLayout(groupLayout);
	}
}
